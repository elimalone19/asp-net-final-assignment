@model IEnumerable<string>


<a asp-action="Index" asp-controller="Home" asp-route-category=""
       asp-route-page="1" >Home</a>


@foreach (string category in Model)
{
    <a class="btn btn-block @(category == ViewBag.Title ? "btn-primary" : "btn-outline-secondary")"
       asp-action="Index" asp-controller="Home" asp-route-category="@category"
       asp-route-page="1" >
        @category
    </a>
}